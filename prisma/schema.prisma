// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./devs.db"
}

model Products {
  id       String   @id @default(uuid())
  title    String   @unique
  desc     String
  price    Decimal
  image    String
  createAt DateTime @default(now())
}
